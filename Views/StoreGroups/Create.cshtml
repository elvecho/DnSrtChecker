@model DnSrtChecker.Models.ViewModels.StoreGroupViewModel

@{
    ViewData["Title"] = "Create";
}


    <div class="container">
        <div class="row">
            <div class="col-sm-4">
                <a onclick="history.go(-1);" class="btn btn-manage"><i class="fa fa-chevron-left iconColorStyleRetex"></i></a>
                <h2 class="titlePage">Nuovo Insegna</h2>
                <hr />
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4">
                <form asp-action="Create">
                    <div asp-validation-summary="All" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="LStoreGroupId" class="control-label">Codice dell'insegna:</label>
                        <input asp-for="LStoreGroupId" class="form-control" name="LStoreGroupId" />
                        <span asp-validation-for="LStoreGroupId" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="SzDescription" class="control-label">Descrizione</label>
                        <input asp-for="SzDescription" class="form-control" />
                        <span asp-validation-for="SzDescription" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Salva" class="btn btn-manage" />
                    </div>
                </form>
            </div>
        </div>

        <div>
            @*<a asp-action="Index" style="float:left;"><i class="fa fa-chevron-left iconColorRetex"></i></a>*@
        </div>
    </div>
